{
  "name": "tower-tc-runner",
  "version": "0.0.1",
  "description": "Test case runner for apiway.io",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "pod": "node ./bin/pod.js",
    "test": "mocha --opts ./mocha.opts test/*.spec.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "add-module-exports",
        "transform-es2015-modules-umd"
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "dependencies": {
    "apiway.js": "latest",
    "babel-register": "^6.7.2",
    "babel-core": "^6.7.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "3.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "aws-sdk": "^2.41.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "morgan": "~1.7.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^7.3.0",
    "ansi_up": "1.3.0",
    "async": "2.3.0",
    "mongoose": "^4.8.3",
    "mocha": "^3.1.2",
    "must": "^0.13.1",
    "promise": "^7.1.1",
    "express": "~4.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApiWay/tower-tc-runner.git"
  },
  "keywords": [
    "mocha",
    "mochajs",
    "apiway"
  ],
  "author": "bluehackmaster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ApiWay/tower-tc-runner/issues"
  },
  "homepage": "https://github.com/ApiWay/tower-tc-runner#readme"
}
